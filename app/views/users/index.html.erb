<h1>All Users</h1>

<div class="login-status">

  <%= "You are logged in as #{current_user.name}" if current_user %>
</div>

<% @users.each do |user| %>
  <h6><%= user.name %></h6>
  <h6><%= user.email %></h6>
  <p></p>
<% end %>
<%= link_to "Sign Up", new_user_path %>
<%= link_to "Log in", login_path %>
