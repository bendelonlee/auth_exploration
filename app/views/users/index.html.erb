<h1>All Users</h1>

<div class="login-status">
  <% if cu = current_user %>
    <%= "You are logged in as #{cu.name}" %>
    <%= button_to "Log out", logout_path, method: :delete %>
  <% else %>
    "You are logged out"
  <% end %>
</div>

<% @users.each do |user| %>
  <h6><%= user.name %></h6>
  <h6><%= user.email %></h6>
  <p></p>
<% end %>
<%= link_to "Sign Up", new_user_path %>
<%= link_to "Log in", login_path %>
